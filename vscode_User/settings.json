{
  "editor.rulers": [80, 90],
  "editor.tabSize": 2,
	"editor.mouseWheelScrollSensitivity": 0.5,
	"files.exclude": {
		"**/.git": true,
    "node_modules": true,
		"**/.DS_Store": true
	},
  "files.trimTrailingWhitespace": true,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": true
}