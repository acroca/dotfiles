{
  "editor.rulers": [
    100,
    120
  ],
  "editor.tabSize": 2,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "files.insertFinalNewline": true,
  "git.confirmSync": false,
  "search.useIgnoreFiles": true,
  "explorer.openEditors.visible": 0,
  "files.associations": {
    "Makefile.*": "makefile",
    "Dockerfile-*": "dockerfile"
  },
  "go.gotoSymbol.ignoreFolders": [
    "vendor"
  ],
  "go.enableCodeLens": {
    "references": false,
    "runtest": false
  },
  "go.gopath": "/home/albert/go",
  "go.lintTool": "golangci-lint",
  "go.lintOnSave": "off",
  "go.vetOnSave": "off",
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "go.languageServerExperimentalFeatures": {
    "diagnostics": true // for diagnostics as you type
  },
  "[go]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
  },
  "gopls": {
    "usePlaceholders": true, // add parameter placeholders when completing a function
  },
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "editor.suggestSelection": "first",
  "window.zoomLevel": 0,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.eol": "\n",
  "workbench.iconTheme": "vscode-icons",
  "window.menuBarVisibility": "toggle",
  "editor.fontSize": 12,
  "java.home": "/usr/lib/jvm/jdk1.8.0_212",
  "java.jdt.ls.vmargs": "-noverify -Xmx1G -XX:+UseG1GC -XX:+UseStringDeduplication -javaagent:\"/home/albert/.vscode/extensions/gabrielbb.vscode-lombok-0.9.8/server/lombok.jar\"",
  "java.test.message.hintForSetingDefaultConfig": false,
  "editor.fontLigatures": true,
  "vsicons.dontShowNewVersionMessage": true,
  "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "workbench.colorTheme": "Material Theme Darker High Contrast",
}
